{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DevLab"
		},
		"AzSQLDBdevLab_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzSQLDBdevLab'"
		},
		"AzureBlobStorage_products_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage_products'"
		},
		"AKV_DevlabKeys_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://MyDevlabKeys.vault.azure.net/"
		},
		"adlsWithKeyVault_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsgen2devlab.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DevLab_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzSQLDBdevLab_connectionString": {
						"value": "[parameters('AzSQLDBdevLab_connectionString')]"
					},
					"AzureBlobStorage_products_connectionString": {
						"value": "[parameters('AzureBlobStorage_products_connectionString')]"
					},
					"AKV_DevlabKeys_properties_typeProperties_baseUrl": {
						"value": "[parameters('AKV_DevlabKeys_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "DevLab_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"adlsWithKeyVault_properties_typeProperties_url": {
						"value": "[parameters('adlsWithKeyVault_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'DevLab_ArmTemplate_0')]"
			]
		}
	]
}